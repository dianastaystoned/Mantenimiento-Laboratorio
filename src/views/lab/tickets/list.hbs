<div class="home-content">
    <h1 class="text-center">Lista de tickets {{comp.marca}}</h1>
    <div class="overview-boxes">
    {{#each ticket}}
      <div class="box">
        <div class="right-side">
            <div class="box-topic">{{fecha}}</div>
            <div class="number">{{estatus}}</div>
            <div class="number">{{estado}}</div>
            <div class="indicator">
              <span class="text">{{problematica}}</span>
            </div>
            <div class="indicator">
              <span class="text">Usuario: {{user_id}}</span>
            </div>
        </div><br><br>
      </div>
    {{/each}}
    </div>
</div>